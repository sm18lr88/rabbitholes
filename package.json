{
  "name": "rabbithole-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "backend:dev": "cd backend && yarn dev",
    "backend:build": "cd backend && yarn build",
    "backend:start": "cd backend && yarn start",
    "frontend:dev": "cd frontend && yarn start",
    "frontend:build": "cd frontend && yarn build",
    "frontend:start": "cd frontend && yarn start",
    "dev": "concurrently \"yarn backend:dev\" \"yarn frontend:dev\"",
    "build": "yarn frontend:build && yarn backend:build",
    "start": "yarn start:sequential",
    "start:backend": "cd backend && yarn start",
    "start:frontend": "cd frontend && yarn start",
    "start:sequential": "yarn start:backend & npx wait-on http://localhost:3000 && yarn start:frontend & npx wait-on http://localhost:5173 && npx open http://localhost:5173"
  },
  "devDependencies": {
    "wait-on": "^7.0.1",
    "open": "^9.0.0",
    "@types/bonjour": "^3.5.13",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-brush": "^3.0.6",
    "@types/d3-chord": "^3.0.6",
    "@types/d3-contour": "^3.0.6",
    "@types/d3-delaunay": "^6.0.4",
    "@types/d3-dsv": "^3.0.7",
    "@types/d3-fetch": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-format": "^3.0.4",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-path": "^3.1.1",
    "@types/d3-polygon": "^3.0.2",
    "@types/d3-quadtree": "^3.0.6",
    "@types/d3-random": "^3.0.3",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-time-format": "^4.0.3",
    "@types/geojson": "^7946.0.16",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "concurrently": "9.1.2",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "6.0.0-rc.1"
  },
  "dependencies": {
    "@types/dagre": "^0.7.52",
    "@types/gsap": "^1.20.2",
    "dagre": "^0.8.5",
    "gsap": "^3.12.7"
  }
}
